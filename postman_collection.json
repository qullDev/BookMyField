            "body": {
              "mode": "raw",
              "raw": "{\n    \"booking_id\": \"{{booking_id}}\"\n}"
            },
            "url": {
              "raw": "{{base_url}}/api/v1/checkout",
              "host": ["{{base_url}}"],
              "path": ["api", "v1", "checkout"]
            }
          }
        },
        {
          "name": "Stripe Webhook (Simulate)",
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              },
              {
                "key": "Stripe-Signature",
                "value": "test_signature"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n    \"id\": \"evt_test_webhook\",\n    \"object\": \"event\",\n    \"type\": \"checkout.session.completed\",\n    \"data\": {\n        \"object\": {\n            \"id\": \"cs_test_session\",\n            \"payment_status\": \"paid\",\n            \"metadata\": {\n                \"payment_id\": \"{{payment_id}}\"\n            }\n        }\n    }\n}"
            },
            "url": {
              "raw": "{{base_url}}/api/v1/webhook",
              "host": ["{{base_url}}"],
              "path": ["api", "v1", "webhook"]
            }
          }
        }
